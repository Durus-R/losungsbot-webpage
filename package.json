{
  "name": "purim-web",
  "version": "0.0.1",
  "description": "Purim Web Frontend",
  "productName": "Purim Web",
  "author": "Rene Regensbogen <rene.regensbogen@mainquest.org>",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.ts,.vue ./",
    "format": "prettier --write \"**/*.{js,ts,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "test": "echo \"No test specified\" && exit 0",
    "dev": "quasar dev",
    "build": "quasar build",
    "build:pwa": "quasar build -m pwa",
    "deploy": "vercel",
    "deploy:prod": "vercel --prod",
    "dev:pwa": "quasar dev -m pwa",
    "icons": "node scripts/icongen.js && icongenie generate -m pwa -i dist/icongen/icon.png -b dist/icongen/wallpaper.png"
  },
  "dependencies": {
    "@quasar/extras": "^1.0.0",
    "@vercel/analytics": "^1.3.1",
    "axios": "^1.2.1",
    "csv-parser": "^3.0.0",
    "papaparse": "^5.4.1",
    "pinia": "^2.0.11",
    "quasar": "^2.6.0",
    "sanitize-html": "^2.13.0",
    "vue": "^3.0.0",
    "vue-router": "^4.0.0"
  },
  "devDependencies": {
    "@quasar/app-vite": "^1.0.0",
    "@quasar/icongenie": "^4.0.0",
    "@types/node": "^12.20.21",
    "@types/papaparse": "^5.3.7",
    "@types/sanitize-html": "^2.11.0",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "autoprefixer": "^10.4.2",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-vue": "^9.0.0",
    "prettier": "^2.5.1",
    "sharp": "^0.33.4",
    "svg2img": "^1.0.0-beta.2",
    "typescript": "^4.5.4",
    "vercel": "^30.2.1",
    "workbox-build": "^7.0.0",
    "workbox-cacheable-response": "^7.0.0",
    "workbox-core": "^7.0.0",
    "workbox-expiration": "^7.0.0",
    "workbox-precaching": "^7.0.0",
    "workbox-routing": "^7.0.0",
    "workbox-strategies": "^7.0.0"
  },
  "engines": {
    "node": "^20 || ^18 || ^16 || ^14.19",
